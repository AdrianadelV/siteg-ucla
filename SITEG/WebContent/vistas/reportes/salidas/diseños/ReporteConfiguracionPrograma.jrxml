	<queryString>
		<![CDATA[SELECT p.nombre as programa, a.nombre as area_investigacion, r.nombre as requisito, i.nombre as item_evaluacion,c.nombre as condicion,pc.valor as valor
  FROM programa p, lapso lp, area_investigacion a, requisito r, item_evaluacion as i, condicion c,
  programa_area pa, programa_item pi, programa_requisito pr, condicion_programa pc
  WHERE p.id = $P{programa} and lp.id = $P{lapso} and p.id = pa.programa_id and a.id = pa.area_investigacion_id and
   lp.id = pa.lapso_id and p.id = pr.programa_id and r.id = pr.requisito_id and lp.id = pr.lapso_id
   and p.id = pi.programa_id and i.id = pi.item_evaluacion_id and lp.id = pi.lapso_id and p.id = pc.programa_id and c.id = pc.condicion_id and
  lp.id = pc.lapso_id GROUP BY p.nombre, a.nombre,lp.nombre,r.nombre,i.nombre,c.nombre,pc.valor;]]>
	</queryString>
