						<image
						src="/public/imagenes/iconos/filtro.png" />
						<textbox instant="true" width="90%" id="txtDescripcionMostrarArea"
							 />
					</auxheader>
					
				
				</auxhead>
			<listhead>
				<listheader label="Nombre" align="center"/>
				<listheader label="DescripciÃ³n" align="center"/>
			
			</listhead>
		

			<template name="model">
				<listitem>
					<listcell label="${each.nombre}" />
					<listcell label="${each.descripcion}" />
				
			</listitem>
