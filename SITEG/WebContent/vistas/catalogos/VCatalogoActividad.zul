<window title="Catalogo Actividad" border="normal" width="800px"
	id="wdwCatalogoActividad" apply="controlador.CCatalogoActividad"
	closable="true">

	<style src="/public/css/bootstrap.css" />

	<style src="public/css/style.css" />

	<listbox id="ltbActividad" width="100%" mold="paging" pageSize="10"
		emptyMessage="No hay actividades registradas">
		<auxhead sclass="category-center">
			<auxheader colspan="1">
				<image src="/public/imagenes/iconos/filtro.png" />
				<textbox instant="true" width="90%"
					id="txtNombreMostrarActividad" />
			</auxheader>
			<auxheader colspan="1">
				<image src="/public/imagenes/iconos/filtro.png" />
				<textbox instant="true" width="90%"
					id="txtDescripcionMostrarActividad" />
			</auxheader>
		</auxhead>
		<listhead>
			<listheader label="Nombre" align="center" />
			<listheader label="DescripciÃ³n" align="center" />
		</listhead>


		<template name="modelo">
			<listitem>
				<listcell label="${each.nombre}" />
				<listcell label="${each.descripcion}" />
			</listitem>
		</template>
	</listbox>
</window>
