	<queryString>
		<![CDATA[SELECT
     s."descripcion" AS titulo,
     tem."nombre" AS tematica,
     p."nombre" AS nombre_tutor,
     p."apellido" AS apellido_tutor,
     est."nombre" AS nombre_estudiante,
     est."apellido" AS apellido_estudiante,
     s."estatus" AS estatus
FROM
     "solicitud_tutoria" s INNER JOIN "solicitud_tutoria_estudiante" se ON s."id" = se."solicitud_id"
     INNER JOIN "estudiante" est ON se."estudiante_cedula" = est."cedula"
     INNER JOIN "programa" prog ON est."programa_id" = prog."id"
     INNER JOIN "profesor" p ON s."profesor_cedula" = p."cedula"
     INNER JOIN "tematica" tem ON s."tematica_id" = tem."id"
     INNER JOIN "area_investigacion" ai ON tem."area_investigacion_id" = ai."id"
WHERE
     prog.id = $P{programa}
    and tem.id = $P{tematica}
     and s.fecha BETWEEN $P{fechainicio} AND $P{fechafin}
     and s.estatus = $P{estatus};]]>
	</queryString>
