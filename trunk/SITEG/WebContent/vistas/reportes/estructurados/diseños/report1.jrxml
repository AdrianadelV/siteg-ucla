	<queryString>
		<![CDATA[SELECT p.nombre, a.nombre as area_investigacion, r.nombre as requisito, i.nombre as item_evaluacion,c.nombre as condicion,pc.valor as valor
  FROM programa p, lapso lp, area_investigacion a, requisito r, item_evaluacion as i, condicion c, programa_area pa, programa_item pi, programa_requisito pr, condicion_programa pc
  WHERE p.id = $P{programa} and lp.id = $P{lapso} and p.id = pa.programa_id and lp.id = pa.lapso_id and p.id = pr.programa_id and lp.id = pr.lapso_id
   and p.id = pi.programa_id and lp.id = pi.lapso_id and p.id = pc.programa_id and
